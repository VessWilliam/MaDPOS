<script>
    toastr.options = {
        closeButton: true,
        progressBar: true,
        positionClass: "toast-top-center",
        timeOut: "5000"
    };

    @if (TempData["Success"] != null)
    {
        <text>toastr.success('@TempData["Success"]'); </text>
    }

    @if (TempData["Error"] != null)
    {
        <text>toastr.error('@TempData["Error"]'); </text>
    }
</script>